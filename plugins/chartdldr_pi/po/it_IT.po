#, c-format
msgid ""
"%d out of %d charts failed to download.\n"
"Check the list, verify there is a working Internet connection and repeat the "
"operation if needed."
msgstr ""
"%d su %d carte non sono state scaricate. \n"
"Controllare l'elenco, verificare la connessione Internet e ripetere "
"l'operazione, se necessario."
#
#, c-format
msgid "%lu charts total, %lu updated, %lu new"
msgstr "%lu carte totali, %lu aggiornate, %lu nuove"
#
#, c-format
msgid "%lu charts total, %lu updated, %lu new, %lu selected"
msgstr "%lu grafici totale, %lu aggiornati, %lu nuovi, %lu selezionati"
#
#, c-format
msgid "%s"
msgstr "%s"
#
#, c-format
msgid "%s (%s to %s), river miles %3.1f - %3.1f"
msgstr "%s (%s a %s), miglia dalla sorgente %3.1f - %3.1f"
#
#, c-format
msgid "%u charts total, %u updated, %u new"
msgstr "%u carte totali, %u aggiornate, %u nuove"
#
#, c-format
msgid "%u charts total, %u updated, %u new, %u selected"
msgstr "%u grafici totale, %u aggiornati, %u nuovi, %u selezionati"
#
msgid "(Please update first)"
msgstr "(Si prega di aggiornare prima)"
#
msgid "Abort download"
msgstr "Annulla download"
#
msgid "Add Catalog"
msgstr "Aggiungi Catalogo"
#
msgid "Add a new chart catalog."
msgstr "Aggiungere un nuovo catalogo carte."
#
msgid "After catalog update select for download"
msgstr "Dopo l'aggiornamento del catalogo, seleziona per il download"
#
msgid "All new charts"
msgstr "Tutte le nuove carte"
#
msgid "All updated charts"
msgstr "Tutte le carte aggiornate"
#
msgid "Allow bulk update of all configured chart sources and charts"
msgstr ""
"Consentire l'aggiornamento di massa di tutte le sorgenti configurate e delle "
"carte"
#
msgid "Cancel"
msgstr "Annulla"
#
msgid "Catalog"
msgstr "Catalogo"
#
msgid "Catalog is not valid."
msgstr "Catalogo non valido."
#
msgid "Chart Catalogs Available"
msgstr "Cataloghi delle carte disponibili"
