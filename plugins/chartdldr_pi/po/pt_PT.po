#, c-format
msgid "Error, the URL to the chart (%s) data seems wrong."
msgstr "Erro, o URL para os dados das cartas (%s) parece errado."
#
msgid "Error, the URL to the chart source data seems wrong."
msgstr ""
"Erro, o URL para a fonte de dados de actualização de cartas parece errado."
#
#, c-format
msgid ""
"Failed to Download Catalog: %s \n"
"Verify there is a working Internet connection."
msgstr ""
"Falha a descarregar Catálogo: %s \n"
"Verifique que existe uma conexão à Internet activa."
#
#, c-format
msgid "Failed to Find New Catalog: %s "
msgstr "Falha a procurar novo Catálogo: %s "
#
#, c-format
msgid ""
"Failed to download: %s \n"
"Verify there is a working Internet connection."
msgstr ""
"Erro a descarregar: %s \n"
"Verifique que existe uma ligação à internet a funcionar."
#
#, c-format
msgid "Failed to save: %s "
msgstr "Erro a guardar: %s "
