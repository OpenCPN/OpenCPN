msgid "URL"
msgstr "URL"
#
msgid "Unknown"
msgstr "Desconhecido"
#
msgid "Up to date"
msgstr "Actualizado"
#
msgid "Update"
msgstr "Atualizar"
#
msgid "Update All"
msgstr "Actualizar tudo"
#
msgid "Update chart source catalog"
msgstr "Actualizar catálogo de origem de cartas"
